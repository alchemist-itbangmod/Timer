{
  "presets": [
    "next/babel",
    "stage-0"
  ],
  "plugins": [
    "inline-dotenv",
    "transform-decorators-legacy",
    "transform-inline-environment-variables",
    [
      "styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ]
  ],
  "env": {
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
    }
  }
}